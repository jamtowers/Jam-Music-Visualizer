{
  "manifest_version": 2,
  "name": "Music Visualizer for YouTubeâ„¢ Music",
  "version": "10.1",
  "description": "Music Visualizer for YouTube Music and YouTube Video. Press ' F2 ' once loaded into a webpage to start visualizing!",
  "web_accessible_resources": ["images/Bar_Viz.png","images/Wave_Viz.png","images/Circle_Viz.png","images/Ambient_Viz.png"],
  "icons": {                   
    "48": "images/icon48.png",                   
    "128": "images/icon128.png"
  },
 "browser_action": {
    "default_icon": {                   
      "48": "images/icon48.png"    
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*", "https://music.youtube.com/*"],
      "css":["visualizerCSS.css"],
      "js": ["visualizer.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}