{
  "manifest_version": 3,
  "name": "Music Visualizer for YouTubeâ„¢ Music",
  "version": "11",
  "description": "Music Visualizer for YouTube Music and YouTube Video. Press ' F2 ' once loaded into a webpage to start visualizing!",
  "web_accessible_resources": [
    {
      "resources": [
        "main.js",
        "visualizer.js",
        "shortcuts.js",
        "images/*",
        "visualizers/*",
        "ui/*",
        "shared/*"
      ],
      "matches": [
        "https://www.youtube.com/*",
        "https://music.youtube.com/*"
      ]

    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://music.youtube.com/*"
      ],
      "css": [
        "visualizer.css"
      ],
      "js": [
        "initializer.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}